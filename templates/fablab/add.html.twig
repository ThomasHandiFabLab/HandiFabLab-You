{% extends 'base.html.twig' %}

{% block title %}Hello FabLabController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>



    <div class="container my-4">
        <h1 class="text-center text-uppercase my-4"><img style="width: 75%;" src="{{ asset('images/logotitre.jpg') }}"></h1>
        <div class="my-2">
            {{ form_row( form.name) }}
        </div>
        <div class="my-2">
            {{ form_row( form.address) }}
        </div>
        <div class="my-2">
            {{ form_row( form.city) }}
        </div>
        <div class="my-2">
            {{ form_row( form.cp )}}
        </div>
        <div class="my-2">
            {{ form_row( form.phonenumber )}}
        </div>
        <div class="my-2">
            {{ form_row( form.email )}}
        </div>
        <button class="btn btn-primary">Ajouter</button>
    </div>

{% endblock %}

