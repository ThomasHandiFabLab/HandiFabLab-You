{% extends 'base.html.twig' %}

{% block title %}Ajouter un fablab - {{ parent() }}{% endblock %}

{% block body %}

    <div class="container">
        <div class="row">
            <div class="col-6 text-center my-2">
                {{ form_row( form.name ) }}
            </div>
        </div>
        <div class="row">
            <div class="col-6 text-center my-2">
                {{ form_row( form.address ) }}
            </div>
            <div class="col-6 text-center my-2">
                {{ form_row( form.city ) }}
            </div>
        </div>
        <div class="row">
            <div class="col-6 text-center my-2">
                {{ form_row( form.cp ) }}
            </div>
        </div>
        <div class="row">
            <div class="col-6 text-center my-2">
                {{ form_row( form.nbProject ) }}
            </div>
            <div class="col-6 text-center my-2">
                {{ form_row( form.phonenumber ) }}
            </div>
        </div>
        <div class="row">
            <div class="col-6 text-center my-2">
                {{ form_row( form.email ) }}
            </div>
            <div class="col-6 text-center my-2">
                {{ form_row( form.printernumber ) }}
            </div>
            <div class="row">
                <div class="col-12 text-center my-2">
                    {{ form_row( form.users ) }}
                </div>
                <div class="col-12 text-center my-2">
                    <button class="btn btn-primary mx-4" type="submit">Ajouter</button>
                    {{ form_end( form ) }}
                </div>
            </div>
    </div>



{% endblock %}
