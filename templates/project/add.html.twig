{% extends 'base.html.twig' %}

{% block title %}Ajouter un projet - {{ parent() }}{% endblock %}

{% block body %}

    <div class="container my-4">
        <div class="">
            <div class="">
                <div class="">
                    {{ form_row( form.name ) }}
                </div>
            </div>
        </div>
        <div class="">
            <div class="">
                {{ form_row( form.created_at ) }}
            </div>
            <div class="">
                {{ form_row( form.deadline_at ) }}
            </div>
        </div>
        <div class="">
            <div class="">
                {{ form_row( form.picture ) }}
            </div>
        </div>
        <div class="">
            <div class="">
                {{ form_row( form.weight ) }}
            </div>
            <div class="">
                {{ form_row( form.length ) }}
            </div>
        </div>
        <div class="">
            <div class="">
                {{ form_row( form.width ) }}
            </div>
            <div class="">
                {{ form_row( form.height ) }}
            </div>
        </div>
            <div class="invisible">
                {{ form_row( form.price ) }}
            </div>
            <div class="">
                <div class="">
                    {{ form_row( form.description ) }}
                </div>
                <div class="row">
                    <button class="btn btn-primary ml-auto" type="submit">Ajouter</button>
                    {{ form_end( form ) }}
                </div>
            </div>
    </div>
{% endblock %}